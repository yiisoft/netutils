{
    "name": "yiisoft/network-utilities",
    "type": "library",
    "description": "Network related utilities",
    "keywords": [
        "network",
        "ip",
        "mask"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/network-utilities/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/network-utilities"
    },
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phan/phan": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\NetworkUtilities\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\NetworkUtilities\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "scripts": {
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "config": {
        "sort-packages": true
    }
}
